{% extends '::base.html.twig' %}
{% block body -%}
    <h1>Demo creation</h1>

    <div class="form_error"></div>
    <form method="POST" class="ajaxForm" id="formik" action="{{path('Obrazki_create')}}" {{ form_enctype(form) }}>
        <div id="form_body">
            {% include 'TestAppBundle:Obrazki:form.html.twig' with {'form': form} %}
        </div>
 
       
        {{form_rest(form)}}
    </form>
 
    <ul class="record_actions">
        <li>
            <a href="{{ path('Obrazki') }}">
                Back to the list
            </a>
        </li>
    </ul>       
                
                <div id="error" class="text-danger"></div>
                <div class="loading-progress"></div>
                <div id="info" class="text-info" ></div>
                <div id="infoserver" class="text-info" ></div>
        
            {%for obrazek in obrazki %}
                <img src="{{asset(obrazek.nazwa)}}" />
            {% endfor%}
            
           
{% endblock %}
 
 
{% block javascripts %}
<script type="text/javascript" src="{{ asset('Symfony/web/main.js') }}"></script>
<script type="text/javascript" src="{{asset('jquery.progresstimer.js')}}"></script>
<script type="text/javascript" src="{{asset('bootstrap-filestyle.min.js')}}"> </script>

<script>$(":file").filestyle({buttonName: "btn-primary"});</script>
<script>
    
    initAjaxForm();
       
   
    
    
</script>
{% endblock %}
